{
  "$schema": "https://json.schemastore.org/nest-cli",
  "collection": "@nestjs/schematics",
  "sourceRoot": "apps/ecommerce-microservices/src",
  "compilerOptions": {
    "deleteOutDir": true,
    "webpack": true,
    "tsConfigPath": "apps/ecommerce-microservices/tsconfig.app.json"
  },
  "monorepo": true,
  "root": "apps/ecommerce-microservices",
  "projects": {
    "auth-service": {
      "type": "application",
      "root": "apps/auth-service",
      "entryFile": "main",
      "sourceRoot": "apps/auth-service/src",
      "compilerOptions": {
        "tsConfigPath": "apps/auth-service/tsconfig.app.json"
      }
    },
    "cart": {
      "type": "application",
      "root": "apps/cart",
      "entryFile": "main",
      "sourceRoot": "apps/cart/src",
      "compilerOptions": {
        "tsConfigPath": "apps/cart/tsconfig.app.json"
      }
    },
     "category": {
      "type": "application",
      "root": "apps/category",
      "entryFile": "main",
      "sourceRoot": "apps/category/src",
      "compilerOptions": {
        "tsConfigPath": "apps/category/tsconfig.app.json"
      }
    },
    "inventory": {
      "type": "application",
      "root": "apps/inventory",
      "entryFile": "main",
      "sourceRoot": "apps/inventory/src",
      "compilerOptions": {
        "tsConfigPath": "apps/inventory/tsconfig.app.json"
      }
    },
    "order": {
      "type": "application",
      "root": "apps/order",
      "entryFile": "main",
      "sourceRoot": "apps/order/src",
      "compilerOptions": {
        "tsConfigPath": "apps/order/tsconfig.app.json"
      }
    }
  }
}